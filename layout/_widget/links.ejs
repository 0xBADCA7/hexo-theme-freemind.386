<% if (theme.links.length){ %>
<div class="widget">
	<h4>Links</h4>
	<ul class="blogroll">
	<% for (var i in theme.links){ %>
		<li><i class="<%- theme.links[i].icon %>"></i>&nbsp;&nbsp;<a href="<%- theme.links[i].url %>" title="<%- theme.links[i].intro %>" target="_blank"]);"><%= theme.links[i].title %></a></li>
	<% } %>
	</ul>
</div>
<% } %>
